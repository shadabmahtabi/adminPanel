            <div class="card">
              <div class="card-body">
                <div class="card-header d-flex align-items-center border-0 justify-content-between">
                  <div class="card-title">Results: <%= users.length %> staff member(s)</div>
                  <div class="sortWrapper">
                    <select name="" id="" class="form-control">
                      <option value="">Sort by A-Z</option>
                      <option value="">Sort by A-Z</option>
                      <option value="">Sort by A-Z</option>
                    </select>
                  </div>
                </div>

                <div class="row">
                  <% users.forEach( user => { %>
                    
                      <div class="col-md-4">
                        <a href="/admin/staff/<%= user._id %>" class="card">
                          <img src="<%= user.profilePic.url %>" class="card-img-top" alt="...">
                          <div class="card-body">
                            <h5 class="card-title pb-0"><%= user.name %></h5>
                            <p class="card-text"><%= user.profession %></p>
                          </div>
                        </a>
                      </div>
  
                  <% }) %>
